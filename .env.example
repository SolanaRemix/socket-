# API Configuration
# Port for the backend API server
API_PORT=3001
PORT=3001

# Frontend Development Server Port
VITE_DEV_PORT=3000

# API URL (optional - auto-detected if not set)
# VITE_API_URL=http://localhost:3001/api

# API Port for frontend to connect to (used in dynamic URL construction)
VITE_API_PORT=3001

# AI API Keys (for Oracle Audit and LLM features)
# GEMINI_API_KEY=your_gemini_api_key_here
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub Token (for PR creation and API access)
# GITHUB_TOKEN=your_token_here

# Repository Brain Access Token (if different from GITHUB_TOKEN)
# REPO_BRAIN_ACCESS_TOKEN=your_token_here

# Web3 Configuration (if using Web3 features)
# WEB3_RPC_URL=https://your-secure-rpc-endpoint.com
# WALLET_MNEMONIC=your_wallet_mnemonic_here
